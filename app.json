{
  "expo": {
    "name": "Fashion Store",
    "slug": "Fashion Store",
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-font"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./MyApp/assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./MyApp/assets/images/LOGO.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.balakrishnan2001.myapp"
    },
    "web": {
      "favicon": "./MyApp/assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "08191ee3-467e-4a73-a07a-4e82bdff8f09"
      }
    }
  }
}
